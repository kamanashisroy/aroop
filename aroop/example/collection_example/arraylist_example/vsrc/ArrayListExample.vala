
using aroop;


class SearchableOPPListExample : Replicable {

	public static int main() {
		ArrayList<xtring> array = ArrayList<xtring>();
		xtring x = new xtring.set_static_string("Have a nice day."); // create a xtring
		core.assert(x != null);
		array[0] = x; // It adds xtring containing "Have a nice day" into the array
		core.assert(array[0] == x);
		return 0;
	}
}
